    "lunr-languages": "^1.12.0",
    "next": "13.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-leaflet": "^4.2.1",
    "react-slick": "^0.29.0",
