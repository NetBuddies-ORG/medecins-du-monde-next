{
  "name": "monboreseau",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "scripts": {
    "graphql": "yarn graphql-codegen --config codegen.yml",
    "start": "yarn env-cmd --no-override yarn next dev",
    "build:static": "yarn run build:next",
    "build:pre": "yarn ts-node ./build/scripts/prebuild.ts",
    "build:next": "yarn run build:pre && yarn next build",
    "build:post": "ts-node ./build/scripts/postbuild.ts",
    "build:preview": "ts-node ./build/scripts/postbuild.ts",
    "lint": "next lint",
    "lint:fix": "eslint --fix .",
    "tsc-check": "tsc --noEmit",
    "env": "yarn env-cmd --no-override yarn run",
    "prepare": "husky install"
  },
  "dependencies": {
    "@christopherpickering/react-leaflet-markercluster": "^1.1.0",
    "classnames": "^2.5.1",
    "danger": "^12.3.3",
    "eslint": "^9.12.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-typescript": "^0.14.0",
    "framer-motion": "^11.11.9",
    "i18next": "^23.16.0",
    "i18next-http-backend": "^2.6.2",
    "i18next-resources-to-backend": "^1.2.1",
    "idb": "^8.0.0",
    "leaflet": "^1.9.4",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.14.0",
    "next": "^14.2.30",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.3",
    "react-icons": "^4.11.0",
    "react-leaflet": "^4.2.1",
    "react-slick": "^0.30.2",
    "react-use": "^17.5.1",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.0",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.3.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/leaflet": "^1.9.12",
    "@types/lunr": "^2.3.7",
    "@types/node": "^20.11.1",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "encoding": "^0.1.13",
    "env-cmd": "^10.1.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.11.0",
    "graphql": "^16.9.0",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "husky": "^9.1.6",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "sass": "^1.77.0",
    "server-only-context": "^0.1.0",
    "slick-carousel": "^1.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
